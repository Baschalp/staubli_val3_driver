<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="toJointArrayRad" access="public">
    <Parameters xmlns="http://www.staubli.com/robotics/VAL3/Param/1">
      <Parameter name="x_jJointRx" type="jointRx" xsi:type="element" use="reference" />
      <Parameter name="x_nJointArray" type="num" xsi:type="array" use="reference" />
    </Parameters>
    <Locals>
      <Local name="l_nDeg2Rad" type="num" xsi:type="array" size="1" />
    </Locals>
    <Code><![CDATA[begin
  l_nDeg2Rad = 0.01745329251
  
  // assuming the remaining elements (6-9) have zeros assigned by default
  x_nJointArray[0] = x_jJointRx.j1 * l_nDeg2Rad
  x_nJointArray[1] = x_jJointRx.j2 * l_nDeg2Rad
  x_nJointArray[2] = x_jJointRx.j3 * l_nDeg2Rad
  x_nJointArray[3] = x_jJointRx.j4 * l_nDeg2Rad
  x_nJointArray[4] = x_jJointRx.j5 * l_nDeg2Rad
  x_nJointArray[5] = x_jJointRx.j6 * l_nDeg2Rad
end]]></Code>
  </Program>
</Programs>